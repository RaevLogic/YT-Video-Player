  "repository": "git@github.com:RaevLogic/YT-Video-Player.git",
  "homepage": "https://raevlogic.github.io/YT-Video-Player",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
